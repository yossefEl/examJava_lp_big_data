<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="132.0" prefWidth="414.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="societes.controllers.AddSocieteController">
   <children>
      <VBox layoutY="1.0" prefHeight="161.0" prefWidth="414.0">
         <children>
            <HBox prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="106.0" text="Nom de la societe">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="societeName" alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="259.0">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox layoutX="10.0" layoutY="30.0" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="106.0" text="Adresse de la societe">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="societeAdrr" alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="259.0">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="65.0" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Button fx:id="closeAddSocieteDialogButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="98.0" style="-fx-background-color: grey;" text="Annuler" textFill="WHITE" onAction="#closeAddSocieteDialog">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button cancelButton="true" fx:id="addSocieteButton" layoutX="306.0" layoutY="30.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="98.0" style="-fx-background-color: #64BB00;" text="Ajouter" textFill="WHITE" onAction="#addSociete">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
